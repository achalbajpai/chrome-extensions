{
  "manifest_version": 2,
  "name": "Social Redirect",  "version": "1.0",
  "description": "Redirects Instagram to LeetCode and Twitter to GitHub",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}
